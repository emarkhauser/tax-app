<p id="notice"><%= notice %></p>

<h1>Listing Incomes</h1>

<table>
  <thead>
    <tr>
      <th>Cashsales</th>
      <th>Salesreturns</th>
      <th>Totalsales</th>
      <th>Paymentonaccount</th>
      <th>Client</th>
      <th>Note</th>
      <th>Date</th>
      <th>Hst</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @incomes.each do |income| %>
      <tr>
        <td><%= income.cashSales %></td>
        <td><%= income.salesReturns %></td>
        <td><%= income.totalSales %></td>
        <td><%= income.paymentOnAccount %></td>
        <td><%= income.client %></td>
        <td><%= income.note %></td>
        <td><%= income.date %></td>
        <td><%= income.hst %></td>
        <td><%= link_to 'Show', income %></td>
        <td><%= link_to 'Edit', edit_income_path(income) %></td>
        <td><%= link_to 'Destroy', income, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Income', new_income_path %>
